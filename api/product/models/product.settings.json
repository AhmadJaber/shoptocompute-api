      "type": "string"
    },
    "memory": {
      "type": "string"
    },
    "keyboard": {
      "type": "string"
    },
    "battery": {
      "type": "string"
    },
    "adapter": {
      "type": "string"
    },
    "touchpad": {
      "type": "string"
